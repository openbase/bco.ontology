    <!-- http://www.openbase.org/bco/ontology#Connection -->

    <owl:Class rdf:about="http://www.openbase.org/bco/ontology#Connection">
        <rdfs:subClassOf rdf:resource="http://www.openbase.org/bco/ontology#BaseUnit"/>
        <rdfs:comment>A connection is a spatial relationship between two physical areas, like tiles. Therefore a connection is clearly identifiable. In addition there are different types of connection (door, passage, window).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.openbase.org/bco/ontology#ContactState -->

